{
  "name": "traveler-frontend",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "clear-build-folder": "del dist",
    "start": "cross-env NODE_ENV=development webpack serve --mode development --progress",
    "build": "npm run clear-build-folder && cross-env NODE_ENV=production webpack --mode production --progress",
    "watch": "cross-env NODE_ENV=development webpack --mode development --watch",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx . --fix",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "analyze": "webpack --json > analyze.json && webpack-bundle-analyzer analyze.json"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "react"
  ],
  "author": "alex",
  "license": "ISC",
